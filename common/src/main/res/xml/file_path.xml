<paths>
    <!-- 适配 Android 11 的私有目录 -->
    <external-files-path
        name="download"
        path="Download/" />
    <!-- 适配缓存目录 -->
    <cache-path
        name="cache"
        path="." />
    <!-- 适配外部存储根目录 (Android 10 以下) -->
    <external-path
        name="external"
        path="." />
    <!-- 添加externalCacheDir支持 -->
    <external-cache-path
        name="external_cache"
        path="." />
</paths>